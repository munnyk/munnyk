//		int b = 0;
//			int c = ~b;
//			//    ~    按位取反
//			//000000000000000000000000000000
//			//111111111111111111111111111111
//			//源码，反码，补码 源码变反码按位取反（第一位符号位不变），反码加一得补码
//			//负数
//			printf("%d\n", c);//使用的，打印的是源码
//			int d = 10;
//			int e = d++; //后置++,先赋值给e，再++
//			printf("d=%d e=%d\n ", d, e);
//			int f = ++d;
//			printf("d=%d f=%d\n ", d, f);//前置
